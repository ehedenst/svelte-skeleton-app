{
  "name": "svelte-skeleton-app",
  "version": "1.0.0",
  "author": "Erik Hedenstr√∂m",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "start": "parcel index.html",
    "build": "npm run lint && npx prettier --write --plugin-search-dir=. ./**/*.svelte && parcel build --no-source-maps index.html",
    "lint": "eslint --cache --ext .js src/",
    "lint:fix": "eslint --cache --ext .js --fix --rule --rule \"no-debugger: 0\" src/"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/svelte": "^1.9.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "jest": "^24.9.0",
    "jest-transform-svelte": "^2.1.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-svelte": "^4.0.4",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "svelte": "^3.15.0",
    "svelte-preprocess": "^3.2.6"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "jest-transform-svelte"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ],
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/tests/"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
